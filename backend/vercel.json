{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/inngest.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/inngest",
      "dest": "api/inngest.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}